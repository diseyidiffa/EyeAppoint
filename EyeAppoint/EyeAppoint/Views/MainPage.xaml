<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
x:Class="EyeAppoint.Views.MainPage"
Title="Eye Appoint">
    
    <ContentPage.Padding>
        <OnPlatform x:TypeArguments="Thickness" iOS="0,20,0,0" Default="0,0,0,0" />
    </ContentPage.Padding>
    
    <ContentPage.Content>
        
        <StackLayout>
            <WebView x:Name="Browser" WidthRequest="1000" HeightRequest="1000"  Navigating="webOnNavigating"  Navigated="webOnEndNavigating" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
